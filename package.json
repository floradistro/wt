{
  "name": "whaletools-native",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "start:dev": "cp .env.development .env && expo start",
    "start:prod": "cp .env.production .env && expo start",
    "env:dev": "cp .env.development .env && echo '✅ Switched to DEVELOPMENT database'",
    "env:prod": "cp .env.production .env && echo '⚠️  Switched to PRODUCTION database'",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 20",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "@callstack/liquid-glass": "^0.6.0",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.4.4",
    "@react-native-community/slider": "^5.0.1",
    "@react-native-picker/picker": "^2.11.1",
    "@sentry/react-native": "~7.2.0",
    "@shopify/react-native-skia": "2.2.12",
    "@supabase/supabase-js": "^2.81.1",
    "expo": "~54.0.24",
    "expo-av": "~16.0.7",
    "expo-blur": "~15.0.7",
    "expo-camera": "~17.0.9",
    "expo-crypto": "~15.0.7",
    "expo-haptics": "~15.0.7",
    "expo-linear-gradient": "~15.0.7",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.8",
    "lottie-react-native": "^6.7.0",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-vision-camera": "^4.7.3",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@react-native-community/cli": "^20.0.2",
    "@react-native/metro-config": "^0.82.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.57.1",
    "eslint-config-expo": "^10.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-native": "^5.0.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "sharp": "^0.34.5",
    "typescript": "~5.9.2"
  },
  "private": true
}
